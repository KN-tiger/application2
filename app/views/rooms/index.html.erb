<div class='container px-5 px-sm-0'>
  <div class='row'>
    <div class='col-md-9 offset-md-1'>
      <h4>Massage List</h4>
      <table class='table'>
        <thead>
          <tr>
            <th>image</th>
            <th>name</th>
            <th>Last Masssage</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @another_entries.each do |entry| %>
            <tr>
              <td><%= image_tag entry.user.get_profile_image, size: '50x50' %></td>
              <td><%= entry.user.name%></td>
              <td>
                <%= link_to room_path(entry.room) do %>
                  <i class="fas fa-envelope"></i><%= Message.find_by(id: entry.room.message_ids.last)&.body %>
                <%end%>
              </td>
            </tr>
          <%end%>
        </tbody>
      </table>
    </div>
  </div>
</div>